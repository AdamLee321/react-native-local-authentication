{
    "include": ["src/**/*.ts"],
    "compilerOptions": {
      "jsx": "react-native",
      "lib": ["es6"],
      "moduleResolution": "node",
      "strict": true,
      "target": "esnext",
      "resolveJsonModule": true,
      "outDir": "dist",
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "declaration": true
    },
    "exclude": [
      "node_modules"
    ]
  }